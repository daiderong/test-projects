<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="UserLoginLast">

	<sql id="tableName" >
		user_login_last
	</sql>
	
	<sql id="selectColumns">
	select
	token token ,
	account account ,
	user_id userId ,
	from_tag fromTag ,
	login_ip loginIp ,
	created_at createdAt ,
	last_login_at lastLoginAt ,
	effi_at effiAt ,
	logout logout ,
	logout_at logoutAt ,
	login_content loginContent ,
	app_id appId ,
	client_info clientInfo ,
	signature signature     FROM 
    <include refid="tableName"/> t
  	</sql>
  	
  	<!-- 添加 -->
	<insert id="create" parameterType="com.wondersgroup.yfybx.userauth.model.UserLoginLast" useGeneratedKeys="true"  keyProperty="token">
		INSERT INTO <include refid="tableName"/> 
		(token,account,user_id,from_tag,login_ip,created_at,last_login_at,effi_at,logout,logout_at,login_content,app_id,client_info,signature)
		VALUES
		(#{token} ,#{account} ,#{userId} ,#{fromTag} ,#{loginIp} ,#{createdAt} ,#{lastLoginAt} ,#{effiAt} ,#{logout} ,#{logoutAt} ,#{loginContent} ,#{appId} ,#{clientInfo} ,#{signature} )
	</insert>
	
	<!-- 批量 添加 -->
	<insert id="create_BATCH"  parameterType="List" >
		INSERT INTO <include refid="tableName"/> 
		(token,account,user_id,from_tag,login_ip,created_at,last_login_at,effi_at,logout,logout_at,login_content,app_id,client_info,signature)
		VALUES 
		<foreach collection="list" item="item" index="index" separator=",">
			(#{item.token} ,#{item.account} ,#{item.userId} ,#{item.fromTag} ,#{item.loginIp} ,#{item.createdAt} ,#{item.lastLoginAt} ,#{item.effiAt} ,#{item.logout} ,#{item.logoutAt} ,#{item.loginContent} ,#{item.appId} ,#{item.clientInfo} ,#{item.signature} )
		</foreach>
	</insert>
	
	<delete id="deleteById" parameterType="Long">
		DELETE FROM 
		<include refid="tableName"/> 
		WHERE token = #{token}
  	</delete>
	
	<select id="loadAll" resultType="com.wondersgroup.yfybx.userauth.model.UserLoginLast">
		<include refid="selectColumns" />
	</select>
	
	<select id="findById" resultType="com.wondersgroup.yfybx.userauth.model.UserLoginLast" 
	parameterType="Long">
		<include refid="selectColumns"/>   
		WHERE token = #{token}
	</select>
	
	<sql id="pageConditions">
	<where>
			  	<if test="token != null">AND token = #{token}</if>
				  	<if test="account != '' and account != null">AND account = #{account}</if>
				  	<if test="userId != null">AND user_id = #{userId}</if>
				  	<if test="fromTag != '' and fromTag != null">AND from_tag = #{fromTag}</if>
				  	<if test="loginIp != '' and loginIp != null">AND login_ip = #{loginIp}</if>
				  	<if test="createdAt != null">AND created_at = #{createdAt}</if>
				  	<if test="lastLoginAt != null">AND last_login_at = #{lastLoginAt}</if>
				  	<if test="effiAt != null">AND effi_at = #{effiAt}</if>
				  	<if test="logout != null">AND logout = #{logout}</if>
				  	<if test="logoutAt != null">AND logout_at = #{logoutAt}</if>
				  	<if test="loginContent != '' and loginContent != null">AND login_content = #{loginContent}</if>
				  	<if test="appId != '' and appId != null">AND app_id = #{appId}</if>
				  	<if test="clientInfo != '' and clientInfo != null">AND client_info = #{clientInfo}</if>
				  	<if test="signature != '' and signature != null">AND signature = #{signature}</if>
			</where>
	</sql>
	
	<!-- 分页查询 -->
	<select id="findPage" resultType="com.wondersgroup.yfybx.userauth.model.UserLoginLast"
		parameterType="com.wondersgroup.yfybx.userauth.model.UserLoginLast">
		<include refid="selectColumns"/> 
		<include refid="pageConditions"/>
		<if test="orderStr != '' and orderStr != null">
				ORDER BY ${orderStr}
		</if>
	</select>
	
	<select id="findPage_COUNT" resultType="int" parameterType="com.wondersgroup.yfybx.userauth.model.UserLoginLast"> 
		select count(1) from
		<include refid="tableName" />
		<include refid="pageConditions" />
	</select>
	
	<sql id="update_sql">
		UPDATE 
		<include refid="tableName"/> 
		<set>
		  		<if test="account != null">account = #{account} ,</if>
		  		<if test="userId != null">user_id = #{userId} ,</if>
		  		<if test="fromTag != null">from_tag = #{fromTag} ,</if>
		  		<if test="loginIp != null">login_ip = #{loginIp} ,</if>
		  		<if test="createdAt != null">created_at = #{createdAt} ,</if>
		  		<if test="lastLoginAt != null">last_login_at = #{lastLoginAt} ,</if>
		  		<if test="effiAt != null">effi_at = #{effiAt} ,</if>
		  		<if test="logout != null">logout = #{logout} ,</if>
		  		<if test="logoutAt != null">logout_at = #{logoutAt} ,</if>
		  		<if test="loginContent != null">login_content = #{loginContent} ,</if>
		  		<if test="appId != null">app_id = #{appId} ,</if>
		  		<if test="clientInfo != null">client_info = #{clientInfo} ,</if>
		  		<if test="signature != null">signature = #{signature} ,</if>
			</set>
		WHERE token = #{token}
	</sql>
		
	<update id="update" parameterType="com.wondersgroup.yfybx.userauth.model.UserLoginLast">
		<include refid="update_sql"/>
	</update>	
		
	

</mapper>